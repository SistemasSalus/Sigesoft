<script>

    $(document).ready(function () {        

        $('#dtpFechaServicio').editable({
            mode: 'inline'
        });

        obtenerFechaActual();
        //CargarEmpresas_Sedes();
        //SetearEmpresaAndSede();

       //$("#lblEmpresa").on("click", function(){
       //    $("#modal-empresa").modal("show");
       //});

        function ObtenerDataHeadCount(nroDni) {
            $.ajax({
                type: "GET",
                url: "/PruebaRapidaCovid19/GetDatosHeadCount?dni=" + nroDni,
                success: function (data) {
                    $("#txtNombres").val(data.v_NombrePila);
                    $("#txtApePaterno").val(data.v_Apellido);
                    $("#txtApeMaterno").val(data.v_SegundoApellido);

                    $("#txtEmail").val(data.v_Mail);
                    $("#txtCelulares").val(data.v_Telefon);
                    $("#txtEmpleadora").val(data.Empleadora);

                    if (data.v_ClaveSexo == "Masculino") {
                        $('#ddlGenero').selectpicker('val', 1);
                    } else if (data.v_ClaveSexo == null) {
                        $('#ddlGenero').selectpicker('val', 1);
                    }
                    else {
                        $('#ddlGenero').selectpicker('val', 2);
                    }
                    
                    $("#dtpFechaNacimiento").val(moment(data.d_FechaNacimiento).format('DD/MM/YYYY'));
                    $("#txtPuesto").val(data.v_PosicionNombre);

                    if (data.v_Soc == "Backus") {
                        $('#ddlTipoEmpresaCovid').selectpicker('val', 1);
                    }
                    else if (data.v_Soc == "Reparto") {
                        $('#ddlTipoEmpresaCovid').selectpicker('val', 3);
                    }
                    else if (data.v_Soc == "Terceros") {
                        $('#ddlTipoEmpresaCovid').selectpicker('val', 2);
                    }
                    else {
                        $('#ddlTipoEmpresaCovid').selectpicker('val', -1);
                    }

                    $('#ddlSede').selectpicker('val', data.SedeId);

                    $("#lblFechaPruebaRapida").val(data.FechaUltimaCovid);
                    $("#lblResultadoPruebaRapida").val(data.ResultadpUltimaCovid);

                    //setearValoresporDefecto();

                }
            });
        }

        $("#form-datos-personales").submit(function (ev) { ev.preventDefault(); });

        $("#txtNroDocumento").on('change', function () {
            ObtenerDataHeadCount($("#txtNroDocumento").val());
        });

        $("#saveEmpresaSede").on("click", function(){

         
           
        });

        $("#saveRegistro").on("click", function () {
            var bootstrapValidator = $("#form-datos-personales").data('bootstrapValidator');
            bootstrapValidator.validate();
            if (bootstrapValidator.isValid()) {

                if ($("#dtpFechaNacimiento").val() == "") {
                    alert("La fecha es requerida");
                    return;
                }

                bootbox.confirm("¿Está seguro de registrar trabajador?", function (result) {

                    var datos = poblarEntidadRegistroTrabajador();

                    if (result) {
                        $('.modal').modal('show');
                        $.ajax({
                            type: "POST",
                            url: "/PruebaRapidaCovid19/RegitrarTrabajador",
                            data: datos,
                            success: function (data) {
                                if (data == "FECHA INVALIDA") {
                                    alert("ERROR! LA FECHA DE SERVICIO ES INVALIDAD");
                                }
                                $('.modal').modal('hide');
                                LimpiarFormularioRegistro();
                                Filter();
                            }
                        });
                    } else {
                        $.niftyNoty({
                            type: 'danger',
                            icon: 'pli-cross icon-2x',
                            message: 'Se canceló la operación.',
                            container: 'floating',
                            timer: 3000
                        });
                    };
                });
            }
            else {
                console.log("FAIL ENCUESTA");
                return;
            }
        });

        $('#demo-default-modal')
        .on('shown.bs.modal', function () {
            $('#form-datos-personales').find('[name="validaNroDocumento"]').focus();
            obtenerFechaActual();
            //setearValoresporDefecto();
        })
        .on('hidden.bs.modal', function () {
            $('#form-datos-personales').bootstrapValidator('resetForm', true);
            obtenerFechaActual();
        });

    });

    function setearValoresporDefecto(){
        var empresaId = localStorage.getItem('empresaId');
        if (empresaId != "N003-OO000000425") {
            $("#ddlTipoEmpresaCovid").val(1);
            $('#ddlTipoEmpresaCovid').selectpicker('refresh');
            $("#ddlSede").val(102);
            $('#ddlSede').selectpicker('refresh');
            $(".info-backus").hide();
        }
    }

    function CargarEmpresas_Sedes() {
        $("#txtEmpresa").autocomplete({
            minLength: 4,
            delay: 500,
            create: function () { $(this).val(""); },
            source: function (request, response) {

                $.ajax({
                    type: "GET",
                    url: "/PruebaRapidaCovid19/ObtenerEmpresas?nombre="+request.term,
                    dataType: 'json',
                    success: function (json) {
                        var array = json.map((res) => {
                            return { "label": res.v_Name, "value": res.v_OrganizationId}
                        });
                        console.log("ARRAY",array);
                        response(array);
                    }
                });

            },
            open: function () {
                $(this).autocomplete('widget').css('z-index', 10000);
                return false;
            },
            select: function (event, ui) {
                $(this).val(ui.item.label);
                cargarComboSede(ui.item.value);
                cargarComboProtocolos(ui.item.value);
                localStorage.setItem('empresaId', ui.item.value);
                localStorage.setItem('empresaNombre', ui.item.label);
                return false;
            }
        });

        $("#txtSede").autocomplete({
            minLength: 4,
            delay: 500,
        });
    }

    function cargarComboSede(empresaId){
        $("#txtSede").autocomplete({
            minLength: 4,
            delay: 500,
            create: function () { $(this).val(""); },
            source: function (request, response) {

                
                $.ajax({
                    type: "GET",
                    url: "/PruebaRapidaCovid19/ObtenerSedes?empresaId="+empresaId+"&&nombre="+request.term,
                    dataType: 'json',
                    success: function (json) {
                        var array = json.map((res) => {
                            return { "label": res.v_Name, "value": res.v_LocationId}
                        });
                        console.log("ARRAY",array);
                        response(array);
                    }
                });

            },
            open: function () {
                $(this).autocomplete('widget').css('z-index', 10000);
                return false;
            },
            select: function (event, ui) {
                console.log("SELECCION", ui.item.label);
                $(this).val(ui.item.label);
                localStorage.setItem('sedeId', ui.item.value);
                localStorage.setItem('sedeNombre', ui.item.label);
                return false;
            }
        });
     
