    function cargarComboProtocolos(empresaId){
        $("#txtProtocolo").autocomplete({
            minLength: 4,
            delay: 500,
            create: function () { $(this).val(""); },
            source: function (request, response) {                
                $.ajax({
                    type: "GET",
                    url: "/PruebaRapidaCovid19/ObtenerProtocolos?empresaId="+empresaId+"&&nombre="+request.term,
                    dataType: 'json',
                    success: function (json) {
                        var array = json.map((res) => {
                            return { "label": res.v_Name, "value": res.v_ProtocolId}
                        });                        
                        response(array);
                    }
                });

            },
            open: function () {
                $(this).autocomplete('widget').css('z-index', 10000);
                return false;
            },
            select: function (event, ui) {
                $(this).val(ui.item.label);
                localStorage.setItem('protocoloId', ui.item.value);
                localStorage.setItem('protocoloNombre', ui.item.label);
                return false;
            }
        });
     
    }
    
