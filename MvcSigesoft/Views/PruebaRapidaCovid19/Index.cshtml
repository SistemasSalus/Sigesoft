    function validateDocType(typeDocumentId, value) {

        if (typeDocumentId == 1) {
            var ex_regular_dni;
            ex_regular_dni = /^\d{8}(?:[-\s]\d{4})?$/;
            if (ex_regular_dni.test(value) == true) {
                return true;
            } else {
                return false;
            }
        }
        return true;
    }

    function validaDNI(dni) {
        var ex_regular_dni;
        ex_regular_dni = /^\d{8}(?:[-\s]\d{4})?$/;
